        MENUITEM SEPARATOR
        MENUITEM "Exit game\tCtrl+F4",					MENU_QUIT, GRAYED
        MENUITEM "Pause game\tPause",					MENU_PAUSE, GRAYED
        MENUITEM SEPARATOR
